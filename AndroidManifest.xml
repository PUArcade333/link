                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="GetScores0Activity"></activity>
        <activity android:name="GetScores1Activity"></activity>
        <activity android:name="GetScores2Activity"></activity>
        <activity android:name="GetScores3Activity"></activity>
