<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.link"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    
    <application
        android:icon="@drawable/ic_launcher"
        android:label="Princeton Arcade" >
        <activity
            android:name=".HelloAndroid"
            android:label="Princeton Arcade" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Linker"
            android:label="Singleplayer Game Select" >
            <intent-filter>
                <action android:name=".Linker" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HighScores"
            android:label="High Scores" >
            <intent-filter>
                <action android:name=".HighScores" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.squirrel.SquirrelMain"
            android:label="Squirrel Chase" >
